<h2><%= @artist.name %></h2>

<div>
  <% @artist.photos.each do |photo| %>
      <%= image_tag photo.image %>
  <% end %>
</div>

<h3>Songs</h3>
<ul><% @artist.songs.each do |song| %>
  <li><%= song.name %></li>
<% end %>
</ul>

<div>
  <%= form_for @song do |f| %>
      <%= f.label :name %>
      <%= f.text_field :name %>

      <%= f.submit "Create" %>
  <% end %>
</div>